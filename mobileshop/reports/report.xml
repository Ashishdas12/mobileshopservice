<odoo>
        <report
            id="report_card"
            string="sales Card pdf"
            model="sale.sale"
            report_type="qweb-pdf"
            name="mobileshop.sales_report"
            file="mobileshop.sales_report" />

        <report
            id="report_card_xls"
            string="sales execl Card"
            model="sale.sale"
            report_type="xlsx"
            name="mobileshop.sales_card_xlsx"
            file="mobileshop.sales_card_xlsx" />

        <record id="sale_excel_report" model="ir.actions.report">
        <field name="name">sales Excel Report</field>
        <field name="model">sales.report</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">mobileshop.sale_excel_xlsx</field>
        <field name="report_file">mobileshop.sale_excel_xlsx</field>
        <field name="binding_type">report</field>
    </record>

